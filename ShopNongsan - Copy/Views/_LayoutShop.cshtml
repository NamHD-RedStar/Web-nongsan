            <form action="~/Nguoidungs/Nhantin_khachhang">
                <div id="noidung_tinnhan" class="fixed-form_group">
                    <!--Bắt đầu tin-->
                    @{ if (Session["nguoidung"] != null)
                        {
                            var tinnhan = db.Nhantins.Where(s => s.id_khachhang == nguoi.id_nguoidung && s.id_admin == 18).ToList();
                            foreach (var item in tinnhan)
                            {
                                if (item.trangthai == 1)
                                {
                                    <div class="in" style="width: 92%; height: auto; line-height: 22px; padding-top: 4px;">
                                        <div class="chat-img">
                                            <img alt="Avtar" src="https://bootdey.com/img/Content/avatar/avatar2.png">
                                        </div>
                                        <div class="chat-body">
                                            <div class="chat-message">
                                                <p>@item.noidungnhan</p>
                                            </div>
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="out" style="width: 92%; height: auto; line-height: 22px; padding-top: 4px;">
                                        <div class="chat-img">
                                            <img alt="Avtar"  style="height:48px;width:48px" src="data:image/jpg;base64,@nguoi.anh_daidien">
                                        </div>
                                        <div class="chat-body">
                                            <div class="chat-message">
                                                <p>@item.noidungnhan</p>
                                            </div>
                                        </div>
                                    </div>
                                }

                            }
                        }
                        else
                        {
                            <h3 >Hãy đăng nhập để nhắn tin nhé</h3>
                        }


                    }


                    <!--Kết thúc tin-->


                </div>
                <div class="fixed-form_footer">
                    <div class="fixed-btn_group">
                        <input style="border:1px solid; border-radius: 10px; width: 90%;" type="text" name="noidung" id="noidung"  required>
                        <input style=" width: 58px; height: 33px; padding: 2px 3px;" type="submit" value="Gửi" class="fixed-btn_group-item active">
                    </div>
                    <a class="fixed-form_footer-link" href="">Nguyễn Văn Nam</a>
                </div>

            </form>
        </div>
